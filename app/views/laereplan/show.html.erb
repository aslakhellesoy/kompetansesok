<p>
  <span class="enhet-overskrift"><%=  t('overskrifter.hovedområde')%></span><br/>
  <h3 class="tittel"><%=h @laereplan.tittel %></h3>
  <span class="benevning">Uuid:</span>
  <span class="uuid"><%=h @laereplan.uuid %></span><br/>
  <span class="benevning">Kode:</span>
  <span class="kode"><%=h @laereplan.kode %></span>
</p>

<p>
  <h3><%= t("overskrifter.hovedområder") %></h3>
  <ul class="linke-liste">
    <% @laereplan.hovedomraader.each do |hovedomraade| %>
      <li><%= link_to hovedomraade.tittel, hovedomraade_path(hovedomraade) %></li>
    <% end %>
  </ul>
</p>

<p>
  <h3><%= t("overskrifter.kompetansemålsett") %></h3>
  <ul class="linke-liste">
    <% @laereplan.kompetansemaalsett.each do |sett| %>
      <li><%= link_to sett.tittel, kompetansemaalsett_path(sett) %></li>
    <% end %>
  </ul>
</p>

<%= render :partial => 'common/back_button' %>
