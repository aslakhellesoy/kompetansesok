<h1>Listing laereplaner</h1>

<table>
  <tr>
    <th>Uuid</th>
    <th>Kode</th>
    <th>Tittel</th>

  </tr>

<% for laereplan in @laereplaner %>
  <tr>
    <td><%=h laereplan.uuid %></td>
    <td><%=h laereplan.kode %></td>
    <td><%=h laereplan.tittel %></td>
  </tr>
<% end %>
</table>

